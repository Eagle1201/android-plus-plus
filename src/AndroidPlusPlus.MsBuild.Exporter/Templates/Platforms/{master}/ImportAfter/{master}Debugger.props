<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <PropertyGroup>

    <DebuggerConfiguration Condition="'$(DebuggerConfiguration)' == ''">Custom</DebuggerConfiguration>

    <DebuggerTargetApk Condition="'$(DebuggerTargetApk)' == ''">$(OutDir)$(ProjectName)-$(Configuration).apk</DebuggerTargetApk>

    <DebuggerCustomLaunchActivity Condition="'$(DebuggerCustomLaunchActivity)' == ''"></DebuggerCustomLaunchActivity>

    <DebuggerDebugMode Condition="'$(DebuggerDebugMode)' == ''">true</DebuggerDebugMode>

    <DebuggerOpenGlTrace Condition="'$(DebuggerOpenGlTrace)' == ''">false</DebuggerOpenGlTrace>

    <DebuggerKeepAppData Condition="'$(DebuggerKeepAppData)' == ''">true</DebuggerKeepAppData>

    <DebuggerGdbTool Condition="'$(DebuggerGdbTool)' == ''">$(PlatformToolsetPrebuilt)\bin\$(TargetPlatformArchitectureToolchainPrefix)-gdb.exe</DebuggerGdbTool>

    <DebuggerLibraryPaths Condition="'$(DebuggerLibraryPaths)' == ''">$(LibraryPath)</DebuggerLibraryPaths>

  </PropertyGroup>

</Project>
