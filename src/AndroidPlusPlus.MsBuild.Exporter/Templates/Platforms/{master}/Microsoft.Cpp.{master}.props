<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <Import Condition="(exists('$(VCTargetsPath)\Platforms\{master}\ImportBefore'))" Project="$(VCTargetsPath)\Platforms\{master}\ImportBefore\*.props" />

  <!--
    Ensure VS doesn't try compilation/linker steps which are unsuitable for Android.
  -->

  <PropertyGroup>

    <IgnoreImportLibrary>true</IgnoreImportLibrary>

    <GenerateManifest>false</GenerateManifest>

  </PropertyGroup>

  <!--
    Configuration output defaults.
  -->

  <PropertyGroup Condition="'$(ConfigurationType)' == 'Application'">

    <LinkCompiled>true</LinkCompiled>

    <TargetName>$(ProjectName)</TargetName>

    <TargetExt>.apk</TargetExt>

  </PropertyGroup>

  <PropertyGroup Condition="'$(ConfigurationType)' == 'DynamicLibrary'">

    <LinkCompiled>true</LinkCompiled>

    <TargetName>$(ProjectName)</TargetName>

    <TargetExt>.so</TargetExt>

  </PropertyGroup>

  <PropertyGroup Condition="'$(ConfigurationType)' == 'StaticLibrary'">

    <LibCompiled>true</LibCompiled>

    <TargetName>$(ProjectName)</TargetName>

    <TargetExt>.a</TargetExt>

  </PropertyGroup>

  <!--
    Resolve target platform, environment variables, and toolset default properties.
  -->

  <Import Project="$(VCTargetsPath)\Platforms\{master}\{master}.Paths.default.props" />

  <Import Project="$(VCTargetsPath)\Platforms\{master}\PlatformToolsets\{master}.Cpp.$(TargetArchitecture).default.props"/>

  <Import Project="$(VCTargetsPath)\Platforms\{master}\PlatformToolsets\$(PlatformToolset)\{master}.Cpp.$(PlatformToolset).props"/>

  <Import Project="$(VCTargetsPath)\Platforms\{master}\{master}.Toolset.default.props" />

  <Import Condition="(exists('$(VCTargetsPath)\Platforms\{master}\ImportAfter'))" Project="$(VCTargetsPath)\Platforms\{master}\ImportAfter\*.props" />

</Project>

