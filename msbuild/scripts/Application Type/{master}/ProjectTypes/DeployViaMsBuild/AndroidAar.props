<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <!--
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  
  <PropertyGroup Condition="'$(AndroidAarBeforeTargets)' == '' and '$(AndroidAarAfterTargets)' == '' and '$(ConfigurationType)' != 'Makefile'">
  
    <AndroidAarBeforeTargets>AndroidAarSign</AndroidAarBeforeTargets>
    
    <AndroidAarAfterTargets>Link</AndroidAarAfterTargets>
    
  </PropertyGroup>

  <!--
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->

  <ItemDefinitionGroup>

    <AndroidAar>

      <OutputCommandLine Condition="'%(AndroidAar.OutputCommandLine)' == ''">false</OutputCommandLine>

      <TrackerLogDirectory Condition="'%(AndroidAar.TrackerLogDirectory)' == ''">$(TLogLocation)</TrackerLogDirectory>

    </AndroidAar>

  </ItemDefinitionGroup>

  <!--
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->

</Project>
