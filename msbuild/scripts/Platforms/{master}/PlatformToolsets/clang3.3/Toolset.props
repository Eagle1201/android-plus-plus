<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <Import Project="$(VCTargetsPath)\Microsoft.Cpp.Common.props" />

  <PropertyGroup>
    <!-- {master} doesn't require import of standard Win32 libraries -->
    <MinimalCoreWin Condition="'$(MinimalCoreWin)' == ''">false</MinimalCoreWin>
  </PropertyGroup>
  
  <Import Project="$(VCTargetsPath)\Platforms\{master}\Platform.Common.props" />

</Project>
