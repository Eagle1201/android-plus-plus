<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <PropertyGroup>

    <__AndroidSdkPropsImported>true</__AndroidSdkPropsImported>

    <AndroidSdkRoot Condition="'$(AndroidSdkRoot)' == ''">$(ANDROID_SDK)</AndroidSdkRoot>
