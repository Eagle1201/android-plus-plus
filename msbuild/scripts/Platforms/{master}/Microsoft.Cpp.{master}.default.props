    <OutDir Condition="'$(OutDir)'=='' AND '$(SolutionDir)' != ''">$(SolutionDir)$(Platform)\$(Configuration)\</OutDir>
    <OutDir Condition="'$(OutDir)'=='' AND '$(SolutionDir)' == ''">$(IntDir)</OutDir>

    <!-- 
      Set properties which are typically controlled within Microsoft.Cl.Common.props to non-empty values to customise later.
    -->

  </PropertyGroup>

  <Import Project="$(VCTargetsPath)\Platforms\{master}\{master}.Paths.default.props" />

  <Import Project="$(VCTargetsPath)\Platforms\{master}\{master}.AndroidSdk.default.props" />

  <Import Project="$(VCTargetsPath)\Platforms\{master}\{master}.AndroidNdk.default.props" />

</Project>

